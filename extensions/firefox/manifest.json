{
    "manifest_version": 2,
    "name": "Remove FACEIT modal",
    "version": "1.2.2",
    "homepage_url": "https://github.com/shakerrrr/remove-faceit-modal",

    "description": "A small extension that aims to eliminate annoyances with the profile modals on FACEIT.",

    "content_scripts": [
        {
            "matches": ["*://www.faceit.com/*"],
            "js": ["remove-faceit-modal.js"]
        }
    ],

    "icons": {
        "32": "icons/icon_32.png"
    },

    "author": "shaker"
}
